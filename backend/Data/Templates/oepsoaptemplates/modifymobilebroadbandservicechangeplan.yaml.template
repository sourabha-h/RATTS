                        <v1:SpendingCapMin>${spendingcapmin}</v1:SpendingCapMin>
                        <v1:SpendingCapMax>${spendingcapmax}</v1:SpendingCapMax>
                        <v1:SpendingCap>${spendingcap}</v1:SpendingCap>
                        <v1:DepositPayment>
                           <v1:CollectedOn>${collectedon}</v1:CollectedOn>
                           <v1:DepositAmount>${depositamount}</v1:DepositAmount>
                           <v1:WaveOff>${waveoff}</v1:WaveOff>
                           <v1:WaveOffReason>${waveoffreason}</v1:WaveOffReason>
                           <v1:DepositReceiptNo>${receiptno}</v1:DepositReceiptNo>
                           <v1:DepositType>${deposittype}</v1:DepositType>
                           <v1:ServiceType>${servicetype}</v1:ServiceType>
                           <v1:PlanName>${systemplanname}</v1:PlanName>
                           <v1:DepositWaiver>${depositwaver}</v1:DepositWaiver>
                        </v1:DepositPayment>
                     </v1:ChangePlan>
                  </v1:MobileBBService>
               </v1:OEPOrder>
            </v1:ListOfOEPOrder>
         </v1:ProcessModifyOrder>
      </v1:OrderEntryRequest>
   </soapenv:Body>
</soapenv:Envelope>